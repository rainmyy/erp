#category{list-style:none;margin:0 0 10px 12px}
#category a{text-decoration:none}
#category li{margin:5px 10px 0;padding:0 5px}
#category li.cplus{list-style:outside url("/public/images/plus.gif")}
#category li.cminus{list-style:outside url("/public/images/minus.gif")}
.categoryList li{cursor:pointer;list-style-image:none;list-style-type:circle;border-bottom:1px solid #E0E0E0}
.categoryListSpan:hover{background:white;}
.categoryFirst:hover{background:#FFF}

.dbody dl{margin-bottom:6px}
.dbody dt{margin-bottom:2px;padding:2px 0;font-weight:bold;color:#CC3300;border-bottom:1px dotted #6bb93f}
.dbody dd{padding:4px 0;line-height:2em}
.dbody ul{margin-left:45px}
.dbody ol{margin-left:52px}
.dlist dl{margin-bottom:6px}
.dlist dt{margin-bottom:2px;padding:2px 0;font-weight:bold;color:#CC3300;border-bottom:1px dotted #6bb93f}
.label{font-size:7px;display:inline-block;width:30px;font-size:normal;line-height:7px}
.dlist dd{padding:4px 0;line-height:2em}
.dlist ul{margin-left:30px;line-height:1.6em}

.product{margin:6px 0;height:106px;padding:4px;border:1px solid #E0E0E0}
.product dt{border:0;padding:0;margin:0;line-height:1.4em}
.product h2{font-weight:normal;line-height:1em}
.productprofile dd{margin:0;padding:0;line-height:1.5em;font-size:12px}

.productlist{margin:2px;padding:0 2px;border-bottom:1px dotted #D1D1D1}
.productlist dt{border:0;padding:0;margin:0;line-height:1.2em;font-weight:normal}
.productlist h3{font-weight:normal;line-height:1.2em;font-size:12px}

.productlistprofile dd{margin:0;padding:0;line-height:1.3em}
.productShow{float:left;margin:4px;text-align:center}
.pshow{padding:2px;width:395px;font-size:16px;line-height:2em;font-weight:bold}
.pshow dd{border-bottom:1px solid #E0E0E0}
.pshow span{width:80px;display:inline-block;font-weight:normal}
.pshow div{line-height:1.8em}
.price{font-size:16px;color:#CC3300;font-weight:bold}
.pdetail{border:1px solid #D0D0D0;margin-top:10px}
.ptitle{border-bottom:2px solid #D0D0D0;font-weight:bold;font-size:16px;padding:2px 10px;margin-top:10px}
.pcell{margin:3px 0 3px 6px;padding:2px;float:left;width:234px;height:186px;border:1px solid #E0E0E0}
.fcell{margin-left:0}
.pcell dt{height:100px;border-bottom:1px solid #E0E0E0}
.colorList{padding:4px;margin:5px 3px 0;width:auto;border:1px solid gray;cursor:pointer;line-height:20px}
.colorList:hover{border:1px solid black;background-color:gray;color:white}
.currentSelect{color:red;font-weight:bold;}
span.on{background:transparent url("/public/images/on.gif") no-repeat right bottom;border:1px solid red}

.tabs{list-style-type:none;margin:20px 0 0 0;padding:0 0 2px 0;line-height:normal}
.tabs li{display:inline;cursor:pointer;color:#42454a;background-color:#EDEBED;border:1px solid #c9c3ba;border-bottom:0;padding:2px 10px 2px 10px;text-decoration:none}
.tabs li:hover{background-color:#F9F8F7}
.tabs li.selected{color:#000;background-color:#F9F8F7;font-weight:bold;padding:6px 10px 3px 10px}
.tabs li a{text-decoration:none;color:#333}
.tabText{border:1px solid #c9c3ba;padding:10px;background-color:#F9F8F7}
.tabText.hide{display:none}

.filter{margin:4px 0 0;padding:4px 0 0;line-height:1.3em}
.filter h1{font-size:14px;}
.filter dl{overflow:hidden;padding:0;zoom:1;border-top:1px dashed #E7CDAE}
.filter dl.first{border:0;}
.filter dt{float:left;width:50px;text-align:right;margin-right:10px;border:0;font-weight:normal;font-size:12px}
.filter dd{float:left;overflow:hidden;padding:4px}
.filter dd div{float:left;margin-right:10px;line-height:1em;color:black;font-weight:normal;padding:2px}
.filter dd div.selected{background:#666;color:white;padding:2px}
.filter dd a,.filter dd a:link,.filter dd a:visited{text-decoration:none;padding:0 2px}
.filter dd a:hover{background:#666;color:white}
#profile{float:left}
#profile dd{line-height:1.4em;padding:2px 5px;border-bottom:1px dashed #D2D2D2}
#profile dd.last{border:0;text-align:right}
#profile span.ptitle{display:inline-block;width:125px;text-align:right;margin-right:10px;font-weight:bold}
#penlist{margin-bottom:20px}
#penlist dt{border-bottom:1px dotted gray;font-weight:bold;margin:10px 0 4px}
#penlist dd{line-height:1.4em;padding:2px 5px;color:#666}

.floatwin{display:none;background-color:white;border:5px solid #888;padding:4px;position:fixed !important;position:absolute;z-index:999}
.floatwinbg{display:none;background-color:black;width:100%;height:100%;left:0;top:0;filter:alpha(opacity=40);opacity:0.4;position:fixed !important;position:absolute;z-index:998}
#review{font-weight:normal;margin-top:20px}
#review dt{border-bottom:1px dotted gray;color:black}
#review h1{color:#CC0000}

#commentList{margin:1px 0}
.commentList dt{border:1px solid #D0D0D0;border-bottom:1px dotted #E7E7E7;padding:10px;margin:10px 0 0;font-size:12px;line-height:1.4em}
.commentList dd{border:1px solid #D0D0D0;border-top:0;padding:10px}
.commentListReply{margin-left:20px}
.commentListReply dt{border:1px solid #E7E7E7}
.commentListReply dd{border:1px solid #E7E7E7}

#news {margin:10px}
#news dt{float:left;width:580px;border-bottom:1px solid #D0D0D0;padding:5px;height:20px}
#news dd{float:left;width:100px;border-bottom:1px solid #D0D0D0;padding:5px;height:20px}

#cart{position:fixed;display:none;visibility:visible;z-index:99;top:2px;width:120px;border:1px solid #E0E0E0;background-color:white;font-size:12px;line-height:1.5em}
#cart dt{border-bottom:1px solid #E0E0E0;background-color:#F0F0F0;font-size:14px;font-weight:bold;padding:2px}
#cart dd{padding:2px;border-bottom:1px dotted #E0E0E0}
#cart dd a{color:gray}
#cartTotal{text-align:right}

.dotted td{border-bottom:1px dotted gray}
.dashed td{border-bottom:1px dashed gray}
.redDashed td{border-bottom:1px dashed #CC0000}
.redSolid td{border-bottom:1px solid #CC0000}
.double td{border-bottom:1px double gray}
.solid td{border-bottom:1px solid gray}

.progress dt,.progress dd,.continue,.order,.minus,.plus{background:url("/public/images/progress.gif") no-repeat 0 0 transparent}
.progress{background:none repeat scroll 0 0 #FAFAFA;border:1px solid #EDEDED;height:42px;margin:20px 10px 0;padding:10px 100px}
.progress dt.on{background-position:47px 23px;color:#d37630}
.progress dt{background-position:-167px 23px;color:#A9A9A9;float:left;height:42px;overflow:visible;text-align:center;width:110px}
.progress dd.half{background-position:-76px -25px;color:#d37630}
.progress dd{background-position:-152px -25px;color:#A9A9A9;float:left;height:32px;padding:10px 0 0;text-align:center;width:76px;font-size:12px}
.progress dd.on{background-position:0 -25px;color:#d37630}
.continue{width:125px;height:41px;background-position:0 -94px;display:inline-block;text-align:center;overflow:hidden;line-height:200px;margin:0 5px}
.order{width:97px;height:41px;background-position:-126px -94px;display:inline-block;text-align:center;overflow:hidden;line-height:200px;margin:0 5px}
.minus{width:9px;height:10px;background-position:-224px -94px;display:inline-block;vertical-align:middle}
.plus{width:8px;height:9px;background-position:-225px -104px;display:inline-block;vertical-align:middle}

.of{margin:10px 10px 0}
.oftitle{border-bottom:1px dotted gray;font-weight:bold;font-size:14px;}
.ofbody{}
.ofbody ul{margin-left:30px}
.ofbody li{line-height:1.5em}
.note{font-size:12px;color:#707070}
.hidden{display:none}
.show{display:block}
.dpform{margin:5px 0 4px 20px;padding:0 4px 4px;background:white}
.dpform span{border-bottom:1px solid gray;font-weight:bold;padding:0 20px 0 5px}

.submit,.paynow{background:url("/public/images/order.gif") no-repeat 0 0}
.submit{width:160px;height:53px;background-position:0 0;line-height:150px;overflow:hidden;border:0}
.paynow{width:160px;height:53px;background-position:0 -53px;line-height:150px;overflow:hidden;margin:10px 0}
.hlYellow{background-color:#FFFFCC;border:1px solid #DDDD00;padding:5px;margin:4px auto}
.hlRed{background-color:#FDDBCC;border:1px solid #FEAA85;padding:5px;margin:4px auto}
.hlGreen{background-color:#DBFDCC;border:1px solid #AAFE85;padding:5px;margin:4px auto}
.hlOrange{background-color:#FEEBD1;border:1px solid #FEB685;padding:5px;margin:4px auto}

.trackerlist{float:left;width:100px;height:150px;margin:0 4px}
.trackerlist dt{text-align:center;line-height:1em;border:0}
.trackerlist dt a{font-size:10px;font-weight:normal}
.trackerlist dd{text-align:center;border:0;height:130px}