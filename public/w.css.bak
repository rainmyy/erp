html{color:#000;background:#F6F6F6}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0}
body{font:normal 14px "Microsoft YaHei",YaHei,SimSun,arial,helvetica;line-height:1.5em}
table{border-collapse:collapse;border-spacing:0}
img{border:0}
address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal}
ul,ol{margin-left:10px}
li{list-style:none}
caption,th{text-align:left}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}
q:before,q:after{content:''}
abbr,acronym{border:0;font-variant:normal}
sup{vertical-align:text-top}
sub{vertical-align:text-bottom}
input,textarea,select{font-family:inherit;font-size:inherit;font-weight:inherit}
input,textarea,select{*font-size:100%}
legend{margin-left:10px;padding:0 5px}
a,a:link,a:visited{color:black;text-decoration:underline}
a:hover{text-decoration:underline;color:#CC3300}
.clear:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0}
.clear{zoom:1}

#wrap{width:995px;margin:0 auto}
#header{margin-top:1px;border:1px solid #e0e0e0;background-color:black}
#logo a{display:block;background:transparent url("/public/images/logo.gif");width:212px;height:169px;text-indent:-2000px}
#nav{margin-top:2px;white-space:nowrap;border:1px solid #e0e0e0;padding:2px 0}
#nav a{text-decoration:none;border-left:1px solid #D5D5D5;padding:0 10px;font-size:16px}
#search{margin-top:2px;border:1px solid #e0e0e0;padding:2px 5px}
#search a{text-decoration:none}
#member{color:#D5D5D5}
#member a{padding:2px 5px}
#currentnone{margin-top:2px}
#current{margin-top:2px;border:1px solid #e0e0e0;padding:2px 5px}

#body{margin-top:2px}
#left{float:left;width:250px}
#main{float:right;width:732px}

#footer{margin:5px 0;border:1px solid #e0e0e0;color:#445;padding:0 5px;font-size:12px}

.pagination{text-align:center}
.tiny{font-size:10px;font-weight:normal}
.small{font-size:12px;font-weight:normal}
.normal{font-size:14px;font-weight:normal}
.middle{font-size:16px;font-weight:normal}
.big{font-size:18px;font-weight:normal}
.hand{cursor:pointer}
.bold{font-weight:bold}
.darkred{color:#CC3300}
.red{color:red}
.black{color:black}
.gray{color:gray}
.right{float:right}
.left{float:left}
.center{text-align:center}
.q{color:#CC0000;font-weight:normal;font-size:14px}
.hand{cursor:pointer}
.darkred{color:#CC0000}
.tdnone a{text-decoration:none}
.close,.button1,.button2,.button,.like,.dislike,.closing,.search,.continue,.order{background:url("/public/images/bg.gif") no-repeat 0 0}
.close{width:15px;height:15px;background-position:0 0;display:inline-block;text-align:center}
.button1{width:91px;height:21px;background-position:0 -44px;display:inline-block;text-align:center;line-height:21px}
.button2{width:91px;height:21px;background-position:0 -66px;display:inline-block;text-align:center;line-height:21px}
.button{width:100px;height:27px;background-position:0 -88px;display:inline-block;text-align:center;line-height:27px}
.like{width:11px;height:13px;background-position:-16px 0;display:inline-block;vertical-align:middle}
.dislike{width:11px;height:13px;background-position:-28px 0;display:inline-block;vertical-align:middle}
.closing{width:71px;height:21px;background-position:0 -22px;line-height:1.8em}
.closing a{color:white;margin-left:10px}
.search{width:43px;height:21px;background-position:-40px 0;display:inline-block;text-align:center}

.sbody{border:1px solid #E0E0E0;margin:0 0 10px}
.stitle{border-bottom:1px solid #E0E0E0;background-color:#F0F0F0;font-weight:bold;font-size:16px;padding:2px 5px}
.sgtitle{border:1px solid #E0E0E0;background-color:#F0F0F0;font-weight:bold;font-size:16px;padding:2px 5px};

